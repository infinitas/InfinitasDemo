DROP TABLE IF EXISTS `view_counter_views`;
CREATE TABLE `view_counter_views` (
  `id` varchar(36) NOT NULL,
  `model` varchar(50) NOT NULL,
  `foreign_key` varchar(36) NOT NULL,
  `user_id` varchar(36) NOT NULL DEFAULT '0',
  `ip_address` varchar(15) NOT NULL DEFAULT '?',
  `year` varchar(4) NOT NULL,
  `month` int(4) NOT NULL,
  `day` int(4) NOT NULL,
  `hour` int(4) NOT NULL,
  `week_of_year` int(4) NOT NULL,
  `day_of_year` int(4) NOT NULL,
  `day_of_week` varchar(1) NOT NULL,
  `continent_code` varchar(5) NOT NULL,
  `country_code` varchar(2) NOT NULL,
  `country` varchar(100) NOT NULL DEFAULT 'Unknown',
  `city` varchar(100) NOT NULL,
  `referer` text NOT NULL,
  `created` datetime DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `ip_address` (`ip_address`),
  KEY `foreign_key` (`foreign_key`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
